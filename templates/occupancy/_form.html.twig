<ul class="collection with-header blue-grey-text hide " id="form_booking_occupancy">
    <li class="collapsible-header" style="border-bottom:0px;">
        <div class="col s12">
            <h5 class="section-title blue-text"><i class="material-icons">today</i>
                New Occupancy
                <div class="right right-align right-aligned">
                    <a href="javascript:history.back()">
                        <i class="material-icons small grey-text">close</i>
                    </a>
                </div>
            </h5>
        </div>
    </li>
    {#Booking Data#}
    <li class="collection-item lighten-5 grey-text">
        <div class="row">
            <div class="col s12 m12">
                {{ form_start(occupancyForm) }}
{#                {{ form_widget(occupancyForm) }}#}
                <div class="row">
                    <div class="input-field col s8">
                        {{ form_row(occupancyForm.name) }}
                    </div>
                    <div class="input-field col s4">
                        {{ form_row(occupancyForm.birthday) }}
                    </div>
                </div>



                <input type="hidden" id="occupancy_booking" name="occupancy[booking]" value="">
                <button class="btn waves-effect waves-light large light-blue z-depth-0" type="submit" name="action">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{{ button_label|default('Save') }}
                    <i class="material-icons right">send</i>
                </button>
                {{ form_end(occupancyForm) }}
            </div>

        </div>
    </li>
</ul>
