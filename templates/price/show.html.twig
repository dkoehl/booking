{% extends 'base.html.twig' %}

{% block title %}Price{% endblock %}

{% block body %}
    <h1>Price</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ price.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ price.type }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ price.price }}</td>
            </tr>
            <tr>
                <th>Tax</th>
                <td>{{ price.tax }}</td>
            </tr>
            <tr>
                <th>Amount</th>
                <td>{{ price.amount }}</td>
            </tr>
            <tr>
                <th>Deleted</th>
                <td>{{ price.deleted }}</td>
            </tr>
            <tr>
                <th>Hidden</th>
                <td>{{ price.hidden }}</td>
            </tr>
            <tr>
                <th>Crdate</th>
                <td>{{ price.crdate }}</td>
            </tr>
            <tr>
                <th>Tstamp</th>
                <td>{{ price.tstamp }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('price_index') }}">back to list</a>

    <a href="{{ path('price_edit', {'id': price.id}) }}">edit</a>

    {{ include('price/_delete_form.html.twig') }}
