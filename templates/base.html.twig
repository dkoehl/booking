<!DOCTYPE html>
<html lang="de">
<head>
    <meta charset="UTF-8">

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

</head>
<body>

<div class="row">
    <div class="col s1 m1 l1" style="position:absolute; height:100%; bottom:0px; padding:30px; background-color:#e8eaf6;">
        <ul class="center">
            <li><a href="/index" class="btn-floating"><i class="material-icons">home</i></a></li>
            <br>
            <br>
            <li><a href="/room"><i class="material-icons teal-text">hotel</i></a></li>
            <br>
            <li><a href="/booking"><i class="material-icons teal-text">assignment_ind</i></a></li>
            <br>
            <li><a href="/guest"><i class="material-icons teal-text">account_circle</i></a></li>
            <br>
            <li><a href=""><i class="material-icons grey-text">alarm</i></a></li>
            <br>
            <li><a href=""><i class="material-icons grey-text">today</i></a></li>
        </ul>
    </div>
    <div class="col s11 m11 l11">
        <div class="content-container" style="position:absolute; float:right; width:92%; left:8%; padding:20px;">
            {% block body %}{% endblock %}
        </div>
    </div>

</div>



{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</body>
</html>
